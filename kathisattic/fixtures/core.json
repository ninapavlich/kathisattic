        "custom_template": "{% load helpful_tags %}\r\n\r\n{% if page_obj.has_previous or page_obj.has_next %}\r\n<ul class=\"pagination\">\r\n        {% if page_obj.has_previous %}\r\n            <li>\r\n                <a href=\"{% url_add_query page=page_obj.previous_page_number %}\" aria-label=\"Previous\">\r\n                    <span aria-hidden=\"true\">&laquo;</span>\r\n                </a>\r\n            </li>\r\n        {% endif %}\r\n\r\n       \r\n        {% pick_pages page_obj.paginator.num_pages page_obj.number as pagination_range %}\r\n        {% for i in pagination_range %}\r\n\r\n            {% if i %}\r\n            <li {% if page_obj.number = i %}class=\"current\"{% endif %}>\r\n                <a href=\"{% url_add_query page=i %}\">{{i}}</a>\r\n            </li>    \r\n            {% else %}\r\n            <li class=\"inactive\">&hellip;</li>    \r\n            {% endif %}\r\n        {% endfor %}  \r\n\r\n\r\n        {% if page_obj.has_next %}\r\n            <li>\r\n                <a href=\"{% url_add_query page=page_obj.next_page_number %}\" aria-label=\"Next\">\r\n                    <span aria-hidden=\"true\">&raquo;</span>\r\n                </a>\r\n            </li>\r\n        {% endif %}\r\n</ul>\r\n{% endif %}",
