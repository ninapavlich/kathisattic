hname;\r\n            var href = $(link).attr(\"href\");\r\n            if(!link_path || link_path == \"\" || link_path == \"/\" || !href || href == \"\" || href == \"#\"){\r\n                return false;\r\n            }\r\n\r\n            if(link_path.charAt(0)!=\"/\"){\r\n                //has no starting slash. add one for consistency\r\n                link_path = \"/\"+link_path\r\n            }\r\n\r\n            var window_path = window.location.pathname;\r\n            \r\n            if (link_path == window_path) {\r\n                return true;\r\n            }\r\n            \r\n            return false;\r\n        },\r\n\r\n        isParentActive: function(link){\r\n\r\n            //if url is domain.com/group/item\r\n            //then domain.com/group or domain.com/group/ is active parent\r\n            var link_path = link.pathname;\r\n            var href = $(link).attr(\"href\");\r\n                if(!link_path || link_path == \"\" || link_path == \"/\" || !href || href == \"\" || href == \"#\"){\r\n                return false;\r\n            }\r\n\r\n            var window_path = window.location.pathname;\r\n            var are_different = link_path != window_path;\r\n            var match_index = window_path.indexOf(link_path)>=0;\r\n\r\n            if (are_different && match_index) {\r\n                return true;\r\n            }\r\n\r\n            return false;\r\n        },\r\n        isExternal: function(link) {\r\n\r\n            var url = $(link).attr(\"href\");\r\n\r\n            if( typeof(url) == 'undefined'){\r\n                return false;\r\n            }\r\n            \r\n            //test if current domain in url\r\n            var current_domain = window.location.host; //window.location.protocol+\"//\"+\r\n            if(url.indexOf(current_domain) >= 0){\r\n                return false;\r\n            }\r\n\r\n            var match = url.match(/^([^:\\/?#]+:)?(?:\\/\\/([^\\/?#]*))?([^?#]+)?(\\?[^#]*)?(#.*)?/);\r\n            if (typeof match[1] === \"string\" && match[1].length > 0 && match[1].toLowerCase() !== location.protocol) return true;\r\n            if (typeof match[2] === \"string\" && match[2].length > 0 && match[2].replace(new RegExp(\":(\"+{\"http:\":80,\"https:\":443}[location.protocol]+\")?$\"), \"\") !== location.host) return true;\r\n            return false;\r\n        },\r\n        isInternalAnchor: function(link){\r\n            var url = $(link).attr(\"href\");\r\n            if( typeof(url) == 'undefined'){\r\n                return false;\r\n            }\r\n\r\n            return this.isExternal(link) == false && url.indexOf('#') >= 0;\r\n        },\r\n        isMailto: function(link){\r\n            var url = $(link).attr(\"href\");\r\n            if( typeof(url) == 'undefined'){\r\n                return false;\r\n            }\r\n\r\n            return url.toLowerCase().indexOf('mailto') >= 0;\r\n        },\r\n        containsString: function(link, string){\r\n\r\n            //if url is domain.com/group/item\r\n            //then domain.com/group or domain.com/group/ is active parent\r\n            var link_path = link.pathname;\r\n            var href = $(link).attr(\"href\");\r\n                if(!link_path || link_path == \"\" || link_path == \"/\" || !href || href == \"\" || href == \"#\"){\r\n                return false;\r\n            }\r\n\r\n            return link_path.indexOf(string)>=0;\r\n        }\r\n\r\n    };\r\n\r\n    $.fn[pluginName] = function ( options ) {\r\n        return this.each(function () {\r\n            if (!$.data(this, \"plugin_\" + pluginName)) {\r\n                $.data(this, \"plugin_\" + pluginName,\r\n                new LinkManager( this, options ));\r\n            }\r\n        });\r\n    };\r\n\r\n})( jQuery, window, document );\r\n\r\nfunction getParameterByName(name) {\r\n    name = name.replace(/[\\[]/, \"\\\\[\").replace(/[\\]]/, \"\\\\]\");\r\n    var regex = new RegExp(\"[\\\\?&]\" + name + \"=([^&#]*)\"),\r\n        results = regex.exec(location.search);\r\n    return results == null ? \"\" : decodeURIComponent(results[1].replace(/\\+/g, \" \"));\r\n}\r\nfunction not_empty(value){\r\n    return typeof(value) != 'undefined' && value != '';\r\n}\r\n\r\n//$( document ).ready(function() {\r\n//  $(\".selector\").pluginName();\r\n//});\r\n\r\n\r\n\n\n\n/* --- SOURCE: debugitem --- */\n\n/*!\r\n * nina@ninalp.com\r\n */\r\n\r\n\r\n;(function ( $, window, document, undefined ) {\r\n\r\n    // Create the defaults once\r\n    var pluginName = \"debugItem\",\r\n        defaults = {\r\n            itemSelector:'.debug-item',\r\n            itemURLAttribute:'data-edit-url',\r\n            toggleSelector:'.debug-item-toggle'\r\n        };\r\n\r\n    // The actual plugin constructor\r\n    function DebugItem( element, options ) {\r\n        this.element = element;\r\n\r\n        this.options = $.extend( {}, defaults, options) ;\r\n\r\n        this.selectionHandler = this.options.handleSelection || this.defaultSelectionHandler;\r\n        this._defaults = defaults;\r\n        this._name = pluginName;\r\n        this.debug_active = false;\r\n\r\n        this.windows = {};\r\n\r\n        this.init();\r\n    }\r\n\r\n    DebugItem.prototype = {\r\n\r\n        init: function() {\r\n            this.addListeners();\r\n            this.render();\r\n        },\r\n        toggleDebug: function() {\r\n            this.setDebug(!this.debug_active);\r\n        },\r\n        setDebug: function(value) {\r\n            this.debug_active = value;\r\n            this.render()\r\n        },\r\n        selectItem: function(item){\r\n            var url = $(item).attr(this.options.itemURLAttribute);\r\n            \r\n            this.selectionHandler(item, url);\r\n        },\r\n        defaultSelectionHandler: function(item, item_url){\r\n            // window.location = item_url;\r\n            \r\n            if(!item_url in this.windows){\r\n                //Create new window                \r\n                var new_window = this.openWindow(item_url);\r\n            }else{\r\n\r\n                //If closed, re-create window\r\n                var win = this.windows[item_url];\r\n                if (win == null || win.closed) {\r\n                    //it was closed, so re-open\r\n                    var new_window = this.openWindow(item_url);\r\n\r\n                }else{\r\n                    //still open, just switch focus\r\n                    win.focus();\r\n                }\r\n            }        \r\n            \r\n        },\r\n        openWindow: function(item_url){\r\n            var new_window = window.open('', '');\r\n            this.windows[item_url] = new_window;\r\n            setTimeout(function() {\r\n                new_window.location.href = item_url;\r\n            }, 100);\r\n\r\n            return new_window;\r\n        },\r\n        render: function() {\r\n            //Update view\r\n\r\n            if(this.debug_active){\r\n                $(this.options.toggleSelector).addClass(\"active\");\r\n                $(this.options.itemSelector).addClass(\"debug-active\");\r\n            }else{\r\n                $(this.options.toggleSelector).removeClass(\"active\");\r\n                $(this.options.itemSelector).removeClass(\"debug-active\");\r\n            }\r\n        },\r\n\r\n        addListeners: function() {\r\n            //bind events\r\n            var parent_ref = this;\r\n\r\n            $(this.options.toggleSelector).bind(\"click\", function(e){\r\n                e.preventDefault();\r\n                parent_ref.toggleDebug();\r\n            });\r\n            $(this.options.itemSelector).bind(\"click\", function(e){\r\n                if(parent_ref.debug_active){\r\n                    e.preventDefault();\r\n                    parent_ref.selectItem(this);\r\n                }\r\n            });\r\n        },\r\n\r\n        removeListeners: function() {\r\n            //unbind events           \r\n        }\r\n\r\n    };\r\n\r\n    // A really lightweight plugin wrapper around the constructor,\r\n    // preventing against multiple instantiations\r\n    $.fn[pluginName] = function ( options ) {\r\n        return this.each(function () {\r\n            if (!$.data(this, \"plugin_\" + pluginName)) {\r\n                $.data(this, \"plugin_\" + pluginName,\r\n                new DebugItem( this, options ));\r\n            }\r\n        });\r\n    };\r\n\r\n})( jQuery, window, document );\r\n\r\n//$( document ).ready(function() {\r\n//  $(\".selector\").pluginName();\r\n//});\r\n\r\n\r\n\n\n\n/* --- SOURCE: sticky-footer --- */\n\n/*!\r\n * author: nina@ninalp.com\r\n * Very simple sticky footer\r\n */\r\n\r\n;(function ( $, window, document, undefined ) {\r\n    var pluginName = \"stickyFooter\",\r\n        defaults = {\r\n            headerSelector : \"#header\",\r\n            mainSelector : \"#main\",\r\n            footerSelector : \"#footer\",\r\n            fixedHeaderBreakpoint: 768\r\n        };\r\n\r\n    // The actual plugin constructor\r\n    function StickyFooter( element, options ) {\r\n        this.element = element;\r\n\r\n        this.options = $.extend( {}, defaults, options) ;\r\n\r\n        this.winw = $(window).width(),\r\n        this.winh = $(window).height();\r\n\r\n        this._defaults = defaults;\r\n        this._name = pluginName;\r\n\r\n        this.init();\r\n    }\r\n\r\n    StickyFooter.prototype = {\r\n\r\n        init: function() {\r\n            this.updateMeasurements();\r\n            \r\n            this.addListeners();\r\n\r\n            this.render()\r\n        },\r\n        \r\n        render: function() {\r\n            //Update view\r\n            $(this.options.mainSelector).css(\"min-height\", this.remaining_height+\"px\");\r\n            $(this.options.footerSelector).addClass('sticky-footer-inited');\r\n        },\r\n        updateMeasurements: function(){\r\n            this.window_height = $(window).height();\r\n            this.window_width = $(window).width();\r\n            this.header_height = $(this.options.headerSelector).outerHeight(true);\r\n            this.main_height = $(this.options.mainSelector).outerHeight(true);\r\n            this.footer_height = $(this.options.footerSelector).outerHeight(true);    \r\n    \r\n            if(this.window_width < this.options.fixedHeaderBreakpoint){\r\n                this.remaining_height = Math.max(0, this.window_height - this.header_height - this.footer_height);    \r\n            }else{\r\n                this.remaining_height = Math.max(0, this.window_height - this.footer_height - this.header_height);\r\n            }\r\n            \r\n        },\r\n        addListeners: function() {\r\n            //bind events\r\n            var parent = this;\r\n            $(window).bind(\"resize\", function(){\r\n                if( $(window).width()!= parent.winw || $(window).height()!= parent.winh ){\r\n                    parent.winw = $(window).width(),\r\n                    parent.winh = $(window).height();\r\n                    parent.updateMeasurements();\r\n                    parent.render();\r\n                }                \r\n            }); \r\n            $(window).bind(\"load\", function() { \r\n                parent.updateMeasurements();\r\n                parent.render();\r\n            });\r\n        },\r\n\r\n        removeListeners: function() {\r\n            //unbind events    \r\n            var parent = this;      \r\n            $(window).unbind(\"resize\", function(){\r\n                parent.updateMeasurements();\r\n                parent.render();\r\n            });  \r\n            $(window).unbind(\"load\", function() { \r\n                parent.updateMeasurements();\r\n                parent.render();\r\n            });\r\n        }\r\n\r\n    };\r\n\r\n    // A really lightweight plugin wrapper around the constructor,\r\n    // preventing against multiple instantiations\r\n    $.fn[pluginName] = function ( options ) {\r\n        return this.each(function () {\r\n            if (!$.data(this, \"plugin_\" + pluginName)) {\r\n                $.data(this, \"plugin_\" + pluginName,\r\n                new StickyFooter( this, options ));\r\n            }\r\n        });\r\n    };\r\n\r\n})( jQuery, window, document );\n\n\n/* --- SOURCE: kathisattic-bootstrap --- */\n\nfunction initKathisattic(){\r\n    console.log(\"hello kathis attic!\");\r\n    /* Houston, we have javascript */\r\n    $('html').addClass('js');\r\n    \r\n    try {\r\n        var is_lte_IE9 = $(\".lt-ie9\").length > 0 || $(\".ie9\").length > 0;\r\n        if(is_lte_IE9){\r\n            $('input, textarea').placeholder();\r\n        }\r\n    }catch(e) {console.log(\"Error in IE placeholder\")}   \r\n\r\n\r\n    try {\r\n        $(document).debugItem({ \r\n            itemSelector:'.debug-item',\r\n            itemClassPrefix:'debug-item-',\r\n            toggleSelector:'.debug-item-toggle'\r\n        });        \r\n    }catch(e) { console.log(\"Error in debug item\")}  \r\n    \r\n   \r\n    //STICKY FOOTER\r\n    try {\r\n        $(document).stickyFooter();       \r\n    }catch(e) { console.log(\"Error in sticky footer\")}  \r\n    \r\n    \r\n    try {\r\n        $(document).linkManager();    \r\n    }catch(e) { console.log(\"Error in link manager\")}\r\n    \r\n}\r\n\r\n$(document).ready(function() {\r\n    initKathisattic();\r\n});\r\n",
