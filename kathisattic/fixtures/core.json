        "title": "Product > Detail",
        "custom_template": "{% extends 'page-base' %}\r\n\r\n{% block bodyclass %}products product-detail{% endblock %}\r\n\r\n{% block title %}{{object.title}} - {{object.get_price}}{% endblock %}\r\n{% block pagetitle %}<h1>{{object.title}} - {{object.get_price}}</h1>{% endblock %}\r\n\r\n{% block content %}\r\n\r\n{{block.super}}\r\n\r\n\r\n<section>\r\n    \r\n    {% if object.sale_status == 'for-sale' %}\r\n    <p><strong>Price</strong> {{object.get_price}}</p>\r\n    \r\n    {% elif object.sale_status == 'draft' %}\r\n    <p><strong>Price</strong> {{object.get_price}} <strong>(Not Yet For Sale)</strong></p>\r\n    \r\n    {% elif object.sale_state == 'sold' %}\r\n        {% if request.user.is_superuser %}\r\n            <p><strong>Sold</strong> for ${{object.sale_price}}</p>\r\n        % else %\r\n        <p><strong>Sold</strong></p>\r\n        {% endif %}\r\n    {% elif object.sale_state == 'donated' %}\r\n        <p><strong>Donated</strong></p>\r\n    {% endif %}\r\n    \r\n    {% if request.user.is_superuser %}\r\n        <p><strong>ID</strong> {{object.key|upper}}</p>\r\n    {% endif %}\r\n    \r\n    <p><strong>Condition</strong> {{object.get_condition_display}}</p>\r\n    \r\n     {% if object.length or object.width or object.height %}<p><strong>Dimensions</strong> {% if object.length %}Length: {{object.length}}{% endif %} {% if object.width %}Width: {{object.width}}{% endif %} {% if object.height %}Height: {{object.height}}{% endif %}</p>{% endif %}\r\n    \r\n    {% if object.weight %}<p><strong>Weight</strong> {{object.weight}}</p>{% endif %}\r\n    \r\n    {% with tags=object.tags.all %}\r\n        {% if tags|length > 0 %}\r\n            <h3>Tags</h3>\r\n            <ul class=\"tag-list\">\r\n                {% for tag in tags %}\r\n                    <li><a href=\"{{tag.get_absolute_url}}\">{{tag}}</a></li>\r\n                {% endfor %}\r\n        \r\n            </ul>\r\n        {% endif %}\r\n    {% endwith %}\r\n    \r\n    {% with related=object.related_products.all %}\r\n        {% if related|length > 0 %}\r\n            <h3>Related Items for Sale</h3>\r\n            <ul class=\"product-list\">\r\n                {% for product in related %}\r\n                    <li>\r\n                        {% include 'product-partials-product' with object=product %}\r\n                    </li>\r\n                {% endfor %}\r\n        \r\n            </ul>\r\n        {% endif %}\r\n    {% endwith %}\r\n    \r\n</section>\r\n\r\n\r\n{% if request.user.is_superuser %}\r\n<div class=\"clearfix\">\r\n    <a href=\"{% url 'product_add_view' %}\" class=\"btn btn-primary pull-right\">Add a product <span class=\"fa fa-plus\"></span></a>\r\n</div>\r\n{% endif %}\r\n\r\n{% endblock %}",
