        "custom_source": "@mixin clearfix() {\r\n    &:before,\r\n    &:after {\r\n        content: \"\";\r\n        display: table;\r\n    }\r\n    &:after {\r\n        clear: both;\r\n    }\r\n}\r\n@mixin text-selection{\r\n  ::selection {\r\n    background: $highlight;\r\n    color:$white;\r\n  }\r\n  ::-moz-selection {\r\n    background: $highlight;\r\n    color:$white;\r\n  }\r\n}\r\n@mixin font-icon(){\r\n  font-family: 'FontAwesome';\r\n\r\n  speak: none;\r\n  font-style: normal;\r\n  font-weight: normal;\r\n  font-variant: normal;\r\n  text-transform: none;\r\n  line-height: 1;\r\n\r\n  /* Better Font Rendering =========== */\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n}\r\n@mixin pointy-hand(){\r\n  cursor: pointer;\r\n  cursor: hand;\r\n}\r\n@mixin opacity($opacity) {\r\n  opacity: $opacity;\r\n  $opacity-ie: $opacity * 100;\r\n  filter: alpha(opacity=$opacity-ie); //IE8\r\n}\r\n",
        "compiler": "scss"
    },
    "model": "core.cssresource",
    "pk": 7
},
