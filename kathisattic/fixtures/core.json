        "title": "Partials > Analytics",
        "custom_template": "{% if not donottrack %}\r\n    {% if not request.user.is_staff %}\r\n        <!-- Analytics -->\r\n        <script>\r\n          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){\r\n          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),\r\n          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)\r\n          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');\r\n        \r\n          ga('create', 'UA-0000000-1', 'www.kathisattic.com');\r\n          ga('require', 'linkid', 'linkid.js');\r\n          ga('send', 'pageview');\r\n        \r\n        </script>\r\n    {% else %}\r\n    <!-- Analytics skipped for admin staff -->\r\n    <script>var ga=function(){}</script>\r\n    {% endif %}\r\n{% else %}\r\n<!-- Analytics skipped for DoNotTrack Request -->\r\n<script>var ga=function(){}</script>\r\n{% endif %}\r\n",
