        "custom_source": "/*!\r\n * author: nina@ninalp.com\r\n * A class to encapsulate common link management\r\n */\r\n\r\n\r\n;(function ( $, window, document, undefined ) {\r\n\r\n    \r\n\r\n    // Create the defaults once\r\n    var pluginName = \"linkManager\",\r\n        defaults = {\r\n            activeClass: \"active\",\r\n            parentActiveClass : \"parent-of-active\",\r\n            externalClass : \"external\",\r\n            mailtoClass : \"mailto\",\r\n            openLinksInNewWindow : true,\r\n            scrollToAnchors: true\r\n        };\r\n\r\n    // The actual plugin constructor\r\n    function LinkManager( element, options ) {\r\n        this.element = element;\r\n\r\n        \r\n        this.options = $.extend( {}, defaults, options) ;\r\n\r\n        this._defaults = defaults;\r\n        this._name = pluginName;\r\n\r\n\r\n        this.init();\r\n    }\r\n\r\n    LinkManager.prototype = {\r\n\r\n        init: function() {\r\n            \r\n            var parent = this;\r\n            $(this.element).find( 'a' ).each(function() {\r\n                    \r\n                var active = parent.isLinkActive(this);\r\n                var parent_active = parent.isParentActive(this);\r\n                var external = parent.isExternal(this);\r\n                var is_anchor = parent.isInternalAnchor(this);\r\n                var mailto = parent.isMailto(this);\r\n\r\n                //console.log(this+\" external? \"+external)\r\n\r\n                if(active){ \r\n                    $(this).addClass(parent.options.activeClass);\r\n                }\r\n                if(parent_active){ \r\n                    $(this).addClass(parent.options.parentActiveClass);\r\n                }\r\n\r\n                if(parent.options.openLinksInNewWindow == true && external==true && mailto == false){\r\n                    $(this).addClass(parent.options.externalClass);\r\n                    $(this).attr(\"target\", \"_blank\");\r\n                }\r\n                if(mailto == true){\r\n                    $(this).addClass(parent.options.mailtoClass);   \r\n                }    \r\n\r\n                if(parent.options.scrollToAnchors == true && is_anchor==true){\r\n                \r\n                    $(this).bind(\"mousedown\", function(event){\r\n                        parent.anchorClicked(event);\r\n                    });\r\n                }\r\n                \r\n            });\r\n                \r\n\r\n        },\r\n        \r\n        anchorClicked:function(event){\r\n            var current_hash = window.location.hash;\r\n            var href = $(this).attr(\"href\");\r\n            if(typeof(href)=='undefined'){\r\n                return;\r\n            }\r\n            var hash = href.substring(href.indexOf('#'));\r\n            if(hash && current_hash!=hash){\r\n\r\n                var offset_y = not_empty($(this).data('scroll-offset')) ? $(this).data('scroll-offset') : 0;\r\n\r\n                event.preventDefault(); \r\n                var dest = 0;\r\n                if ($(this.hash).offset().top > $(document).height() - $(window).height()) {\r\n                    dest = $(document).height() - $(window).height() - offset_y;\r\n                } else {\r\n                    dest = $(this.hash).offset().top - offset_y;\r\n                }\r\n                //go to destination\r\n                $('html,body').animate({\r\n                    scrollTop: dest\r\n                }, 500, 'swing');\r\n            }\r\n        },\r\n        isLinkActive:function(link){\r\n\r\n            var link_path = link.pathname;\r\n            var href = $(link).attr(\"href\");\r\n            if(!link_path || link_path == \"\" || link_path == \"/\" || !href || href == \"\" || href == \"#\"){\r\n                return false;\r\n            }\r\n\r\n            if(link_path.charAt(0)!=\"/\"){\r\n                //has no starting slash. add one for consistency\r\n                link_path = \"/\"+link_path\r\n            }\r\n\r\n            var window_path = window.location.pathname;\r\n            \r\n            if (link_path == window_path) {\r\n                return true;\r\n            }\r\n            \r\n            return false;\r\n        },\r\n\r\n        isParentActive: function(link){\r\n\r\n            //if url is domain.com/group/item\r\n            //then domain.com/group or domain.com/group/ is active parent\r\n            var link_path = link.pathname;\r\n            var href = $(link).attr(\"href\");\r\n                if(!link_path || link_path == \"\" || link_path == \"/\" || !href || href == \"\" || href == \"#\"){\r\n                return false;\r\n            }\r\n\r\n            var window_path = window.location.pathname;\r\n            var are_different = link_path != window_path;\r\n            var match_index = window_path.indexOf(link_path)>=0;\r\n\r\n            if (are_different && match_index) {\r\n                return true;\r\n            }\r\n\r\n            return false;\r\n        },\r\n        isExternal: function(link) {\r\n\r\n            var url = $(link).attr(\"href\");\r\n\r\n            if( typeof(url) == 'undefined'){\r\n                return false;\r\n            }\r\n            \r\n            //test if current domain in url\r\n            var current_domain = window.location.host; //window.location.protocol+\"//\"+\r\n            if(url.indexOf(current_domain) >= 0){\r\n                return false;\r\n            }\r\n\r\n            var match = url.match(/^([^:\\/?#]+:)?(?:\\/\\/([^\\/?#]*))?([^?#]+)?(\\?[^#]*)?(#.*)?/);\r\n            if (typeof match[1] === \"string\" && match[1].length > 0 && match[1].toLowerCase() !== location.protocol) return true;\r\n            if (typeof match[2] === \"string\" && match[2].length > 0 && match[2].replace(new RegExp(\":(\"+{\"http:\":80,\"https:\":443}[location.protocol]+\")?$\"), \"\") !== location.host) return true;\r\n            return false;\r\n        },\r\n        isInternalAnchor: function(link){\r\n            var url = $(link).attr(\"href\");\r\n            if( typeof(url) == 'undefined'){\r\n                return false;\r\n            }\r\n\r\n            return this.isExternal(link) == false && url.indexOf('#') >= 0;\r\n        },\r\n        isMailto: function(link){\r\n            var url = $(link).attr(\"href\");\r\n            if( typeof(url) == 'undefined'){\r\n                return false;\r\n            }\r\n\r\n            return url.toLowerCase().indexOf('mailto') >= 0;\r\n        },\r\n        containsString: function(link, string){\r\n\r\n            //if url is domain.com/group/item\r\n            //then domain.com/group or domain.com/group/ is active parent\r\n            var link_path = link.pathname;\r\n            var href = $(link).attr(\"href\");\r\n                if(!link_path || link_path == \"\" || link_path == \"/\" || !href || href == \"\" || href == \"#\"){\r\n                return false;\r\n            }\r\n\r\n            return link_path.indexOf(string)>=0;\r\n        }\r\n\r\n    };\r\n\r\n    $.fn[pluginName] = function ( options ) {\r\n        return this.each(function () {\r\n            if (!$.data(this, \"plugin_\" + pluginName)) {\r\n                $.data(this, \"plugin_\" + pluginName,\r\n                new LinkManager( this, options ));\r\n            }\r\n        });\r\n    };\r\n\r\n})( jQuery, window, document );\r\n\r\nfunction getParameterByName(name) {\r\n    name = name.replace(/[\\[]/, \"\\\\[\").replace(/[\\]]/, \"\\\\]\");\r\n    var regex = new RegExp(\"[\\\\?&]\" + name + \"=([^&#]*)\"),\r\n        results = regex.exec(location.search);\r\n    return results == null ? \"\" : decodeURIComponent(results[1].replace(/\\+/g, \" \"));\r\n}\r\nfunction not_empty(value){\r\n    return typeof(value) != 'undefined' && value != '';\r\n}\r\n\r\n//$( document ).ready(function() {\r\n//  $(\".selector\").pluginName();\r\n//});\r\n\r\n\r\n",
