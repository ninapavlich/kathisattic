        "custom_source": ".debug-container{\r\n    padding: 0.5em;\r\n    color:$gray-dark;\r\n}\r\n@mixin debug-item-hover(){\r\n  position:relative;\r\n  &:before{\r\n    content: 'EDIT';\r\n    position:absolute;\r\n    top:0px;\r\n    right:0px;\r\n    font-size: 11px;\r\n    padding: 0.4em 1em 0.2em;\r\n    background: $highlight;\r\n    color:#fff;\r\n    z-index: 1000;\r\n  } \r\n}\r\n.debug-item{\r\n  &.debug-active{\r\n    border:1px dashed #ccc !important;\r\n    cursor: pointer;\r\n    cursor: hand;\r\n\r\n    &:hover{\r\n      border:1px solid $highlight !important;\r\n      @include debug-item-hover();     \r\n    }    \r\n  }\r\n}",
